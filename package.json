{
  "name": "saralayoga",
  "version": "1.0.0",
  "description": "Sarala Yoga Website - Professional yoga training in Bangalore",
  "scripts": {
    "dev": "npm run dev:css",
    "dev:css": "tailwindcss -i ./src/css/styles.css -o ./dist/styles.css --watch",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tailwindcss -i ./src/css/styles.css -o ./dist/styles.css --minify",
    "build:minify": "node build.js",
    "serve": "python3 -m http.server 3000"
  },
  "devDependencies": {
    "cssnano": "^6.0.1",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0"
  },
  "keywords": [
    "yoga",
    "wellness",
    "bangalore",
    "fitness"
  ],
  "author": "Sarala Yoga",
  "license": "ISC"
}

